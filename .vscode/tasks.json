				},
				"background": {
					"activeOnStart": true,
					"beginsPattern": ".*vite.*",
					"endsPattern": ".*ready in.*"
				}
			},
			"presentation": {
				"focus": true,
				"panel": "dedicated"
			}
		}
	]
}
