{
  "include": [
    "src/**/*.ts",
    "src/**/*.d.ts",
    "./typed-router.d.ts"
  ],
  "compilerOptions": {
    "noEmit": true,
    "target": "ES2020",
    "lib": [
      "ES2020",
      "DOM"
    ],
    "strict": true,
    "isolatedModules": true,
    "module": "preserve",
    "moduleResolution": "bundler",
    "skipLibCheck": true,
    "types": ["unplugin-vue-router/client"]
  }
}
